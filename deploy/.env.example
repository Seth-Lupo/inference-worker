# =============================================================================
# Voice Agent Environment Configuration
# Copy to .env and fill in values
# =============================================================================

# HuggingFace Token (required for downloading Qwen3, CosyVoice)
# Get from: https://huggingface.co/settings/tokens
HF_TOKEN=hf_xxxxxxxxxxxxxxxxxxxxxxxxxxxx

# NGC API Key (for pulling NVIDIA containers)
# Get from: https://ngc.nvidia.com/setup/api-key
NGC_API_KEY=

# GPU Configuration
CUDA_VISIBLE_DEVICES=0

# Triton Configuration
TRITON_LOG_LEVEL=INFO

# Worker Configuration
LOG_LEVEL=INFO
