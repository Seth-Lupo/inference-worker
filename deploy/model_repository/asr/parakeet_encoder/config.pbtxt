# Parakeet Encoder - Native ONNX Backend
# Input: Mel features [batch, 128, time]
# Output: Encoded features [batch, time/4, 512]

name: "parakeet_encoder"
platform: "onnxruntime_onnx"
max_batch_size: 8

input [
  {
    name: "audio_signal"
    data_type: TYPE_FP16
    dims: [ 128, -1 ]
  },
  {
    name: "length"
    data_type: TYPE_INT64
    dims: [ 1 ]
  }
]

output [
  {
    name: "outputs"
    data_type: TYPE_FP16
    dims: [ -1, 512 ]
  },
  {
    name: "outputs_length"
    data_type: TYPE_INT64
    dims: [ 1 ]
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
    gpus: [ 0 ]
  }
]

# ONNX Runtime optimization
optimization {
  execution_accelerators {
    gpu_execution_accelerator: [
      { name: "cuda" }
    ]
  }
}
