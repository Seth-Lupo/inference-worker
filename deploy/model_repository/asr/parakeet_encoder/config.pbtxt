# Parakeet Encoder - ONNX Runtime Python Backend
# Converts mel spectrogram features to encoder hidden states
#
# Input: Mel features [1, 128, time]
# Output: Encoded features [1, time/4, 512]

name: "parakeet_encoder"
backend: "python"
max_batch_size: 0

input [
  {
    name: "audio_signal"
    data_type: TYPE_FP16
    dims: [ 1, 128, -1 ]
  },
  {
    name: "length"
    data_type: TYPE_INT64
    dims: [ 1 ]
  }
]

output [
  {
    name: "outputs"
    data_type: TYPE_FP16
    dims: [ 1, -1, 512 ]
  },
  {
    name: "outputs_length"
    data_type: TYPE_INT64
    dims: [ 1 ]
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
    gpus: [ 0 ]
  }
]

parameters {
  key: "ONNX_DIR"
  value: { string_value: "/models/asr/parakeet_onnx" }
}

version_policy: { latest: { num_versions: 1 } }
