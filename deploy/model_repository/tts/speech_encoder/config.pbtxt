# Chatterbox Speech Encoder - Native ONNX Backend
# Encodes reference audio into speaker embeddings and conditioning
# Source: ResembleAI/chatterbox-turbo-ONNX
#
# Input: Reference audio waveform (24kHz float32)
# Output: Speaker embeddings + conditioning for T3 and decoder

name: "speech_encoder"
platform: "onnxruntime_onnx"
max_batch_size: 0

# Use original filename (has external data file reference)
default_model_filename: "speech_encoder_fp16.onnx"

input [
  {
    name: "audio_values"
    data_type: TYPE_FP32
    dims: [ 1, -1 ]  # [batch, samples] Variable length audio samples
  }
]

output [
  {
    name: "audio_features"
    data_type: TYPE_FP32
    dims: [ -1, -1, 1024 ]  # [batch, time, 1024]
  },
  {
    name: "audio_tokens"
    data_type: TYPE_INT64
    dims: [ 1, -1 ]  # Speech tokens from reference
  },
  {
    name: "speaker_embeddings"
    data_type: TYPE_FP32
    dims: [ 1, -1 ]  # Speaker embedding vector
  },
  {
    name: "speaker_features"
    data_type: TYPE_FP32
    dims: [ -1, -1, 80 ]  # [batch, time, 80] for decoder
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
    gpus: [ 0 ]
  }
]

# ONNX Runtime GPU optimization
optimization {
  execution_accelerators {
    gpu_execution_accelerator: [
      { name: "cuda" }
    ]
  }
}
