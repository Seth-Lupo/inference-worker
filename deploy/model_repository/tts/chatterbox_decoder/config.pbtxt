name: "chatterbox_decoder"
platform: "onnxruntime_onnx"
max_batch_size: 8

# Conditional decoder: speech tokens -> mel spectrogram
# Uses MultiHeadAttention from com.microsoft domain - not TensorRT compatible

input [
  {
    name: "input_ids"
    data_type: TYPE_INT64
    dims: [-1]
  },
  {
    name: "conditioning"
    data_type: TYPE_FP32
    dims: [256]
  }
]

output [
  {
    name: "mel_spectrogram"
    data_type: TYPE_FP32
    dims: [80, -1]
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
  }
]

optimization {
  execution_accelerators {
    gpu_execution_accelerator : [
      { name : "cuda" }
    ]
  }
}

dynamic_batching {
  preferred_batch_size: [1, 2, 4]
  max_queue_delay_microseconds: 100
}
