name: "chatterbox_encoder"
platform: "onnxruntime_onnx"
max_batch_size: 8

input [
  {
    name: "input_features"
    data_type: TYPE_FP32
    dims: [-1, 80]
  }
]

output [
  {
    name: "conditioning"
    data_type: TYPE_FP32
    dims: [256]
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
  }
]

# Use CUDA execution provider for GPU acceleration
optimization {
  execution_accelerators {
    gpu_execution_accelerator : [
      { name : "cuda" }
    ]
  }
}

dynamic_batching {
  preferred_batch_size: [1, 2, 4]
  max_queue_delay_microseconds: 100
}
