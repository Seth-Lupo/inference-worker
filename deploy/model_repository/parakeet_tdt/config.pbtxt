# Parakeet TDT 0.6B V2 - Automatic Speech Recognition
# ONNX/TensorRT model for speech-to-text transcription
#
# Input: 16kHz mono audio (variable length)
# Output: Token IDs for decoding

name: "parakeet_tdt"
backend: "onnxruntime"
max_batch_size: 8

input [
  {
    name: "audio_signal"
    data_type: TYPE_FP32
    dims: [ -1 ]  # Variable length audio
  },
  {
    name: "length"
    data_type: TYPE_INT64
    dims: [ 1 ]  # Audio length in samples
  }
]

output [
  {
    name: "tokens"
    data_type: TYPE_INT64
    dims: [ -1 ]  # Variable length token sequence
  },
  {
    name: "token_timestamps"
    data_type: TYPE_FP32
    dims: [ -1 ]  # Timestamps for each token
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
    gpus: [ 0 ]
  }
]

dynamic_batching {
  preferred_batch_size: [ 1, 2, 4, 8 ]
  max_queue_delay_microseconds: 50000
}

# Model warmup to reduce first inference latency
model_warmup [
  {
    name: "warmup_audio"
    batch_size: 1
    inputs {
      key: "audio_signal"
      value: {
        data_type: TYPE_FP32
        dims: [ 16000 ]  # 1 second of audio
        zero_data: true
      }
    }
    inputs {
      key: "length"
      value: {
        data_type: TYPE_INT64
        dims: [ 1 ]
        zero_data: true
      }
    }
  }
]

version_policy: { latest: { num_versions: 1 } }
